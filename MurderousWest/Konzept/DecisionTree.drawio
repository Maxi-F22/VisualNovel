          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="a_588HEZvl3vcVWjfH1_-20" value="Sheriff: Wie wärs, sollen wir als nächstes zum alten Brunnen, damit du mit deiner Gabe schauen kannst, ob dir etwas auffällt, das wir übersehen haben oder sollen wir zum Friedhof, dem Ort, wo die letzte Leiche gefunden wurde?" style="rounded=1;whiteSpace=wrap;html=1;strokeColor=#FFFFFF;fontColor=#FFFFFF;fillColor=#182E3E;" parent="1" vertex="1">
