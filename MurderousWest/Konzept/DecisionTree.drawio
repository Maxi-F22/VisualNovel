        <mxCell id="oH5baXMhosWWVNXAdIbK-18" value="Sie wollen mich verarschen oder?" style="rounded=0;whiteSpace=wrap;html=1;strokeColor=#FFFFFF;fontColor=#FFFFFF;fillColor=#182E3E;labelBackgroundColor=none;" parent="1" vertex="1">
