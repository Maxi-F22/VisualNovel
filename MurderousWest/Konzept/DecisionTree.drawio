        <mxCell id="a_588HEZvl3vcVWjfH1_-273" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;strokeColor=#23445D;" parent="1" source="a_588HEZvl3vcVWjfH1_-270" target="a_588HEZvl3vcVWjfH1_-272" edge="1">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="a_588HEZvl3vcVWjfH1_-270" value="Wie aus einem Reflex heraus, zieht MC den einzigen Gegenstand, den er an sich trägt, und schwingt ihn in einem großen Bogen. Der Hammer trifft Deputy genau an der Schläfe und sie sackt zusammen.&lt;br&gt;&lt;br&gt;MC: Oh Gott das wollte ich nicht, es war ein Reflex.&lt;br&gt;&lt;br&gt;Der Sheriff eilt auft sie zu, aber sie hat bereits zu viel Blut verloren." style="rounded=1;whiteSpace=wrap;html=1;strokeColor=#FFFFFF;fontColor=#FFFFFF;fillColor=#182E3E;" parent="1" vertex="1">
          <mxGeometry x="-5290" y="7080" width="280" height="200" as="geometry" />
        </mxCell>
        <mxCell id="a_588HEZvl3vcVWjfH1_-272" value="Mittleres Ende: Die Mörderin ist tot und &lt;br&gt;ihr Blut klebt an MCs Händen" style="rhombus;whiteSpace=wrap;html=1;fillColor=#ffe6cc;labelBackgroundColor=none;strokeColor=#d79b00;" parent="1" vertex="1">
          <mxGeometry x="-5630" y="7078" width="240" height="204" as="geometry" />
        </mxCell>
        <mxCell id="a_588HEZvl3vcVWjfH1_-279" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;strokeColor=#23445D;" parent="1" source="a_588HEZvl3vcVWjfH1_-274" target="a_588HEZvl3vcVWjfH1_-278" edge="1">
