        <mxCell id="a_588HEZvl3vcVWjfH1_-22" value="Gibt es noch etwas anderes Auffälliges?" style="rounded=0;whiteSpace=wrap;html=1;strokeColor=#FFFFFF;fontColor=#FFFFFF;fillColor=#182E3E;" parent="1" vertex="1">
